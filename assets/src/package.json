{
  "name": "@project/theme-bundler",
  "version": "1.0.0",
  "description": "Frontend asset bundler theme setup.",
  "author": "peytz",
  "license": "ISC",
  "keywords": [
    "pnpm",
    "webpack",
    "wp-scripts"
  ],
  "homepage": "https://gitlab.peytz.dk/wp/pelican",
  "repository": {
    "type": "git",
    "url": "https://gitlab.peytz.dk/wp/pelican"
  },
  "engines": {
    "node": "v22.14.0"
  },
  "dependencies": {
    "jquery": "3.7.1",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.29",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.4",
    "@types/wordpress__block-editor": "^11.5.15",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@wordpress/blocks": "^12.29.0",
    "@wordpress/browserslist-config": "^5.33.0",
    "@wordpress/i18n": "^4.52.0",
    "@wordpress/icons": "^9.43.0",
    "@wordpress/postcss-plugins-preset": "^4.34.0",
    "@wordpress/scripts": "^27.1.0",
    "browserslist": "^4.22.3",
    "css-loader": "^7.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-import": "^2.29.1",
    "po2json": "^0.4.5",
    "postcss-preset-env": "^9.3.0",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack-remove-empty-scripts": "^1.0.4"
  },
  "scripts": {
    "build": "NODE_ENV=\"production\" pnpm exec wp-scripts build --config \"wp-scripts/webpack/config/index.ts\"",
    "build:dev": "NODE_ENV=\"development\" pnpm exec wp-scripts start --config \"wp-scripts/webpack/config/index.ts\" --no-watch",
    "deps:install": "pnpm install",
    "deps:clean": "rm -rfv \"node_modules\"",
    "format": "pnpm run \"/^format:(css|js)$/\"",
    "format:fix": "pnpm run \"/^format:(css|js):fix$/\"",
    "format:css": "pnpm exec prettier --check \"scss/**/*.{css,less,sass,scss,styl}\"",
    "format:css:fix": "pnpm exec wp-scripts format \"scss/**/*.{css,less,sass,scss,styl}\"",
    "format:js": "pnpm exec prettier --check \"./**/*.{cjs,cts,js,jsx,mjs,svelte,ts,tsx}\"",
    "format:js:fix": "pnpm exec wp-scripts format \"./**/*.{cjs,cts,js,jsx,mjs,svelte,ts,tsx}\"",
    "lint": "pnpm run \"/lint:(css|js)$/\"",
    "lint:fix": "pnpm run \"/lint:(css|js):fix$/\"",
    "lint:css": "pnpm exec wp-scripts lint-style --allow-empty-input \"scss\"",
    "lint:css:fix": "pnpm exec wp-scripts lint-style --allow-empty-input --fix \"scss\"",
    "lint:js": "pnpm exec wp-scripts lint-js --config \"./.eslintrc.js\" \"./**/*.{cjs,cts,js,jsx,mjs,svelte,ts,tsx}\"",
    "lint:js:fix": "pnpm exec wp-scripts lint-js --config \"./.eslintrc.js\" --fix \"./**/*.{cjs,cts,js,jsx,mjs,svelte,ts,tsx}\"",
    "lint:staged": "pnpm exec \"lint-staged\"",
    "packages:update": "pnpm exec wp-scripts packages-update",
    "pnpm:install": "corepack use \"pnpm@latest\"",
    "pnpm:update": "corepack use \"pnpm@*\"",
    "preinstall": "npx only-allow pnpm",
    "start": "NODE_ENV=\"development\" pnpm exec wp-scripts start --env \"development\" --config \"wp-scripts/webpack/config/index.ts\"",
    "translate": "pnpm exec po2json \"../../languages/da_DK.po\" \"../../languages/da_DK-pelican.json\" -f jed1.x"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
